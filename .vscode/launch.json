{
    "name": "Django",
    "type": "python",
    "request": "launch",
    "stopOnEntry": false,
    "pythonPath": "${config.python.pythonPath}",
    "program": "${workspaceRoot}/manage.py",
    "args": [
        "runserver",
        "--no-color",
        "--noreload"
    ],
    "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput",
        "DjangoDebugging"
    ],
    "configurations": [ {
        "name": "Python Debugger: Django",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/uppcl_health_care/manage.py",
        "args": ["runserver"],
        "django": true,
        "justMyCode": true
      }]
}